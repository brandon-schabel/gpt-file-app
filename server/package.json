{
  "name": "server",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "kill-port": "lsof -ti :8080 | xargs kill",
    "link": "bun link @u-tools/core @u-tools/react @u-tools/open-ai",
    "dev": "bun run link && bun run kill-port && bun run index.ts",
    "dev:hot": "bun run link && bun run kill-port && bun --watch index.ts",
    "update-u-tools": "bun update @u-tools/core @u-tools/react @u-tools/open-ai"
  },
  "devDependencies": {
    "bun-types": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@u-tools/core": "^0.0.43",
    "@u-tools/open-ai": "^0.0.6",
    "@u-tools/react": "^0.0.21"
  }
}
